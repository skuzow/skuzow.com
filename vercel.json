{
  "headers": [
    {
      "source": "/(.*).(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).(avif|webp|svg|png|jpg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, stale-while-revalidate=120"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/alejandro-porras-torrecilla"
    },
    {
      "source": "/github",
      "destination": "https://github.com/skuzow"
    }
  ]
}
